{
    "name": "itlo/cms",
    "description": "SkeekS CMS â€” control panel and tools based on php framework Yii2",
    "keywords": ["framework", "cms", "skeeks", "sx", "skeeks cms", "app", "yii2", "extension"],
    "homepage": "https://cms.skeeks.com",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/itlo-cms/cms",
        "issues": "https://github.com/itlo-cms/cms/issues",
        "wiki": "https://cms.itlo.ru/docs",
        "authors": "https://itlo.ru"
    },
    "authors": [
        {
            "name": "itlo",
            "email": "support@itlo.ru",
            "homepage": "http://itlo.ru"
        },
        {
            "name": "Ivan Roman",
            "email": "info@itlo.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",

        "yiisoft/yii2": "^2.0.24 || dev-master@dev",
        "yiisoft/yii2-jui": "^2.0.7",
        "yiisoft/yii2-bootstrap": "^2.0.9 || dev-master@dev",


        "itlo/cms-composer": "^0.1 || dev-master@dev",
        "itlo/cms-composer-update-plugin": "^0.1 || dev-master",

        "kartik-v/yii2-datecontrol": "^1.9.7",
        "kartik-v/yii2-widget-datepicker": "^1.4.7",
        "kartik-v/yii2-widget-datetimepicker": "^1.4.8",
        "kartik-v/yii2-widget-colorinput": "^1.0.5",

        "mihaildev/yii2-elfinder": "^1.3.0",
        "giggsey/libphonenumber-for-php": "^8.10.8",

        "paulzi/yii2-auto-tree": "^1.1.0",
        "paulzi/yii2-adjacency-list": "^2.2.0",
        "paulzi/yii2-nested-sets": "^1.1.0",
        "paulzi/yii2-materialized-path": "^2.1.0",

        "bower-asset/fancybox": "^3.1",
        "bower-asset/jquery": "2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "skeeks\\cms\\":  "src/"
        }
    },
    "extra": {
        "config-plugin": {
            "web": [
                "src/config/common.php",
                "src/config/web.php"
            ],
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ],

            "skeeks-cms-web": [
                "src/config/common.php",
                "src/config/web.php"
            ],
            "skeeks-cms-console": [
                "src/config/common.php",
                "src/config/console.php"
            ]
        }
    }
}
